cars = []
loop = True
while loop == True:
  ask = str(input("enter a car brand or press x to exit"))
  if ask != "x":
    cars.append(ask)
  if ask == "x":
    print("exiting...")
    loop = False
    print("in the list theres", cars,)
  else:
      ask = str(input("enter a car brand or press x to exit"))
      cars.append(ask)
