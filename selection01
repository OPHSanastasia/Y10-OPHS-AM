#Selection

Team1Points = 5
Team2Points = 8
Team3Points = 6
Team4Points = 8

if Team1Points != 4:
  print("Team 1 has",Team1Points)
  
if Team2Points == 8:
  print("Team 2 has",Team2Points)
  
if Team3Points < 8:
  print("Team 3 has",Team3Points)

if Team4Points >= 8:
  print("Team 4 has",Team4Points)
